{
    "name": "serhioli/leetcode",
    "description": "Solved problems for leetcode",
    "license": "Unlicense",
    "type": "project",
    "autoload": {
        "psr-4": {
            "serhioli\\leetcode\\": "src/",
            "serhioli\\leetcode\\tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sergei Lysogor",
            "email": "me@serhioli.net"
        }
    ],
    "config": {
        "platform": {
