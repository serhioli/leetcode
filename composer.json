            "infection/extension-installer": true
        }
    },
    "require": {
        "php": ">=8.0",
        "infection/infection": "~0.26.16",
        "laminas/laminas-code": "^4.7",
        "phpunit/phpunit": "~9.5",
        "symfony/console": "^5.4",
        "symfony/filesystem": "^5.4",
        "symfony/string": "^5.4",
        "vimeo/psalm": "~5.4"
